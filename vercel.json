{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install", 
  "framework": "nextjs",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/",
      "destination": "/chat"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/chat",
      "permanent": true
    }
  ]
}